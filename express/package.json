{
  "name": "discord-study-bot",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "ts-node src/main.ts",
    "start:watch": "nodemon",
    "start:prod": "node dist/main.js",
    "build": "tsc",
    "forever:prod": "forever start dist/main.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [ "src" ],
    "exec": "npm start",
    "ext": "ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.14",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "domain": "^1.0.0",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "forever": "^3.0.0"
  }
}
