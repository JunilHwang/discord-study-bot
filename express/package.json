{
  "name": "discord-study-bot",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "start": "../node_modules/.bin/ts-node ./src/main.ts",
    "start:watch": "../node_modules/.bin/nodemon",
    "start:prod": "node dist/main.js",
    "build": "../node_modules/.bin/tsc",
    "forever:prod": "../node_modules/.bin/forever start ./dist/main.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JunilHwang/discord-study-bot.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JunilHwang/discord-study-bot/issues"
  },
  "homepage": "https://github.com/JunilHwang/discord-study-bot#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.14",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "forever": "^3.0.0"
  }
}
